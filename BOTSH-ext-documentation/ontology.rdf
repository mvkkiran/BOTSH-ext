    <owl:Class rdf:about="http://purl.org/bosh#Appliance">
        <rdfs:subClassOf rdf:resource="http://purl.org/botsh-ext#ElectronicDevices"/>
    </owl:Class>
    


    <!-- http://purl.org/bosh#NetworkComponent -->


    <owl:Class rdf:about="http://purl.org/bosh#NetworkComponent">
        <rdfs:subClassOf rdf:resource="http://purl.org/botsh-ext#Thing"/>
    </owl:Class>
    


    <!-- http://purl.org/bosh#SensingDevice -->


    <owl:Class rdf:about="http://purl.org/bosh#SensingDevice">
        <rdfs:subClassOf rdf:resource="http://purl.org/botsh-ext#Thing"/>
    </owl:Class>
    


    <!-- http://purl.org/botsh-ext#AgriculturalBuilding -->


    <owl:Class rdf:about="http://purl.org/botsh-ext#AgriculturalBuilding">
        <rdfs:subClassOf rdf:resource="http://purl.org/botsh-ext#Building"/>
    </owl:Class>
    


    <!-- http://purl.org/botsh-ext#AnimalBuilding -->


    <owl:Class rdf:about="http://purl.org/botsh-ext#AnimalBuilding">
        <rdfs:subClassOf rdf:resource="http://purl.org/botsh-ext#Building"/>
    </owl:Class>
    


    <!-- http://purl.org/botsh-ext#Arch -->


    <owl:Class rdf:about="http://purl.org/botsh-ext#Arch">
        <rdfs:subClassOf rdf:resource="http://purl.org/botsh-ext#StructuralElement"/>
    </owl:Class>
    


    <!-- http://purl.org/botsh-ext#ArchitecturalElement -->


    <owl:Class rdf:about="http://purl.org/botsh-ext#ArchitecturalElement">
        <owl:disjointWith rdf:resource="http://purl.org/botsh-ext#Zone"/>
        <rdfs:comment>Architectural elements are the unique details and component parts that, together, form the architectural style of houses, buildings and structures.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/botsh-ext#BathRoom -->


    <owl:Class rdf:about="http://purl.org/botsh-ext#BathRoom">
        <rdfs:subClassOf rdf:resource="http://purl.org/botsh-ext#Room"/>
    </owl:Class>
    


    <!-- http://purl.org/botsh-ext#Beam -->


    <owl:Class rdf:about="http://purl.org/botsh-ext#Beam">
        <rdfs:subClassOf rdf:resource="http://purl.org/botsh-ext#StructuralElement"/>
    </owl:Class>
    


    <!-- http://purl.org/botsh-ext#BedRoom -->


    <owl:Class rdf:about="http://purl.org/botsh-ext#BedRoom">
        <rdfs:subClassOf rdf:resource="http://purl.org/botsh-ext#Room"/>
    </owl:Class>
    


    <!-- http://purl.org/botsh-ext#Bridge -->


    <owl:Class rdf:about="http://purl.org/botsh-ext#Bridge">
        <rdfs:subClassOf rdf:resource="http://purl.org/botsh-ext#NonBuilding"/>
