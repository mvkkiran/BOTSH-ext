    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://purl.org/botsh-ext#adjacentElement -->


    <owl:ObjectProperty rdf:about="http://purl.org/botsh-ext#adjacentElement">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/botsh-ext#hasElement"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/botsh-ext#intersectingElement"/>
        <rdfs:comment xml:lang="en">Relation between a zone and its adjacent building elements, bounding the zone.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/botsh-ext#"/>
        <rdfs:label xml:lang="en">adjacent element</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/botsh-ext#adjacentZone -->


    <owl:ObjectProperty rdf:about="http://purl.org/botsh-ext#adjacentZone">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/botsh-ext#Zone"/>
        <rdfs:range rdf:resource="http://purl.org/botsh-ext#Zone"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/botsh-ext#intersectsZone"/>
        <rdfs:comment xml:lang="en">Relationship between two zones that share a common interface, but do not intersect.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/botsh-ext#"/>
        <rdfs:label xml:lang="en">adjacent zone</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
        <schema:domainIncludes rdf:resource="http://purl.org/botsh-ext#Building"/>
        <schema:domainIncludes rdf:resource="http://purl.org/botsh-ext#BuildingStorey"/>
        <schema:domainIncludes rdf:resource="http://purl.org/botsh-ext#Site"/>
        <schema:domainIncludes rdf:resource="http://purl.org/botsh-ext#Space"/>
        <schema:rangeIncludes rdf:resource="http://purl.org/botsh-ext#Building"/>
        <schema:rangeIncludes rdf:resource="http://purl.org/botsh-ext#BuildingStorey"/>
        <schema:rangeIncludes rdf:resource="http://purl.org/botsh-ext#Site"/>
        <schema:rangeIncludes rdf:resource="http://purl.org/botsh-ext#Space"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/botsh-ext#containsElement -->


    <owl:ObjectProperty rdf:about="http://purl.org/botsh-ext#containsElement">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/botsh-ext#hasElement"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://purl.org/botsh-ext#containsZone"/>
            <rdf:Description rdf:about="http://purl.org/botsh-ext#containsElement"/>
        </owl:propertyChainAxiom>
        <rdfs:comment xml:lang="en">Relation to a building element contained in a zone.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/botsh-ext#"/>
        <rdfs:label xml:lang="en">contains element</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/botsh-ext#containsThing -->


    <owl:ObjectProperty rdf:about="http://purl.org/botsh-ext#containsThing">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/botsh-ext#hasThing"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://purl.org/botsh-ext#containsZone"/>
            <rdf:Description rdf:about="http://purl.org/botsh-ext#containsElement"/>
            <rdf:Description rdf:about="http://purl.org/botsh-ext#containsThing"/>
        </owl:propertyChainAxiom>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/botsh-ext#"/>
        <rdfs:label xml:lang="en">contains thing</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/botsh-ext#containsZone -->


    <owl:ObjectProperty rdf:about="http://purl.org/botsh-ext#containsZone">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/botsh-ext#Zone"/>
        <rdfs:range rdf:resource="http://purl.org/botsh-ext#Zone"/>
        <rdfs:comment xml:lang="en">Relationship to the subzones of a major zone. A space zone could for instance be contained in a storey zone which is further contained in a building zone. botsh-ext:containsZone is a transitive property. This implies that in the previous example a botsh-ext:containsZone relationship holds between the space zone and the building zone.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/botsh-ext#"/>
        <rdfs:label xml:lang="en">contains zone</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
        <schema:domainIncludes rdf:resource="http://purl.org/botsh-ext#Building"/>
        <schema:domainIncludes rdf:resource="http://purl.org/botsh-ext#BuildingStorey"/>
        <schema:domainIncludes rdf:resource="http://purl.org/botsh-ext#Site"/>
        <schema:domainIncludes rdf:resource="http://purl.org/botsh-ext#Space"/>
        <schema:rangeIncludes rdf:resource="http://purl.org/botsh-ext#Building"/>
        <schema:rangeIncludes rdf:resource="http://purl.org/botsh-ext#BuildingStorey"/>
        <schema:rangeIncludes rdf:resource="http://purl.org/botsh-ext#Site"/>
        <schema:rangeIncludes rdf:resource="http://purl.org/botsh-ext#Space"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/botsh-ext#deviceMountedOn -->


    <owl:ObjectProperty rdf:about="http://purl.org/botsh-ext#deviceMountedOn">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/botsh-ext#isPartOf"/>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/botsh-ext#"/>
        <ns1:term_status>stable</ns1:term_status>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/botsh-ext#hasBuilding -->


    <owl:ObjectProperty rdf:about="http://purl.org/botsh-ext#hasBuilding">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/botsh-ext#containsZone"/>
        <rdfs:domain rdf:resource="http://purl.org/botsh-ext#Zone"/>
        <rdfs:range rdf:resource="http://purl.org/botsh-ext#Building"/>
        <rdfs:comment xml:lang="en">Relation to buildings contained in a zone. The typical domains of botsh-ext:hasBuilding are instances of botsh-ext:Site.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/botsh-ext#"/>
        <rdfs:label xml:lang="en">has building</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
        <schema:domainIncludes rdf:resource="http://purl.org/botsh-ext#Site"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/botsh-ext#hasElement -->


    <owl:ObjectProperty rdf:about="http://purl.org/botsh-ext#hasElement">
        <rdfs:domain rdf:resource="http://purl.org/botsh-ext#Zone"/>
        <rdfs:range rdf:resource="http://purl.org/botsh-ext#ArchitecturalElement"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://purl.org/botsh-ext#containsZone"/>
            <rdf:Description rdf:about="http://purl.org/botsh-ext#hasElement"/>
        </owl:propertyChainAxiom>
        <rdfs:comment xml:lang="en">Links a Zone to an Element that is either contained in, adjacent to or intersecting with the Zone. The intended use of this relationship is not to be stated explicitly, but to be inferred from its sub-properties. It will, for example, allow one to query for all the doors of a building given that they have an adjacency to spaces of the building.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/botsh-ext#"/>
        <rdfs:label xml:lang="en">has building element</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/botsh-ext#hasMLRun -->


    <owl:ObjectProperty rdf:about="http://purl.org/botsh-ext#hasMLRun">
        <rdfs:domain rdf:resource="http://purl.org/botsh-ext#MicroController"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/mls#Run"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/botsh-ext#hasNetworkInterface -->


    <owl:ObjectProperty rdf:about="http://purl.org/botsh-ext#hasNetworkInterface">
        <rdfs:domain rdf:resource="http://purl.org/toco/Device"/>
        <rdfs:range rdf:resource="http://purl.org/toco/Interface"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/botsh-ext#hasNonBuilding -->


    <owl:ObjectProperty rdf:about="http://purl.org/botsh-ext#hasNonBuilding">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/botsh-ext#containsZone"/>
        <rdfs:domain rdf:resource="http://purl.org/botsh-ext#Zone"/>
        <rdfs:range rdf:resource="http://purl.org/botsh-ext#NonBuilding"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/botsh-ext#hasSpace -->


    <owl:ObjectProperty rdf:about="http://purl.org/botsh-ext#hasSpace">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/botsh-ext#containsZone"/>
        <rdfs:domain rdf:resource="http://purl.org/botsh-ext#Zone"/>
        <rdfs:range rdf:resource="http://purl.org/botsh-ext#Space"/>
        <rdfs:comment xml:lang="en">Relation to spaces contained in a zone. The typical domains of botsh-ext:hasSpace are instances of botsh-ext:BuildingStorey or botsh-ext:Building.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/botsh-ext#"/>
        <rdfs:label xml:lang="en">has space</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
        <schema:domainIncludes rdf:resource="http://purl.org/botsh-ext#BuildingStorey"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/botsh-ext#hasStorey -->


    <owl:ObjectProperty rdf:about="http://purl.org/botsh-ext#hasStorey">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/botsh-ext#containsZone"/>
        <rdfs:domain rdf:resource="http://purl.org/botsh-ext#Zone"/>
        <rdfs:range rdf:resource="http://purl.org/botsh-ext#BuildingStorey"/>
        <rdfs:comment xml:lang="en">Relation to storeys contained in a zone. The typical domains of botsh-ext:hasStorey are instances of botsh-ext:Building.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/botsh-ext#"/>
        <rdfs:label xml:lang="en">has storey</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
        <schema:domainIncludes rdf:resource="http://purl.org/botsh-ext#Building"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/botsh-ext#hasSubElement -->


    <owl:ObjectProperty rdf:about="http://purl.org/botsh-ext#hasSubElement">
        <rdfs:domain rdf:resource="http://purl.org/botsh-ext#ArchitecturalElement"/>
        <rdfs:range rdf:resource="http://purl.org/botsh-ext#ArchitecturalElement"/>
        <rdfs:comment xml:lang="en">Relation between two building elements, either one element hosting another (e.g. a wall hosts a window) or a subcomposition of a building element into smaller parts (e.g. an air handling unit has as a part a fan).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/botsh-ext#"/>
        <rdfs:label xml:lang="en">has sub-element</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/botsh-ext#hasSubThing -->


    <owl:ObjectProperty rdf:about="http://purl.org/botsh-ext#hasSubThing">
        <rdfs:domain rdf:resource="http://purl.org/botsh-ext#Thing"/>
        <rdfs:range rdf:resource="http://purl.org/botsh-ext#Thing"/>
        <rdfs:comment xml:lang="en">One controller can have another controller or sensor as part of it.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/botsh-ext#hasThing -->


    <owl:ObjectProperty rdf:about="http://purl.org/botsh-ext#hasThing">
        <owl:inverseOf rdf:resource="http://purl.org/botsh-ext#isPartOf"/>
        <rdfs:domain rdf:resource="http://purl.org/botsh-ext#ArchitecturalElement"/>
        <rdfs:range rdf:resource="http://purl.org/botsh-ext#Thing"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://purl.org/botsh-ext#containsZone"/>
            <rdf:Description rdf:about="http://purl.org/botsh-ext#hasElement"/>
            <rdf:Description rdf:about="http://purl.org/botsh-ext#hasThing"/>
        </owl:propertyChainAxiom>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/botsh-ext#"/>
        <rdfs:label xml:lang="en">has Thing</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/botsh-ext#interfaceOf -->


    <owl:ObjectProperty rdf:about="http://purl.org/botsh-ext#interfaceOf">
        <rdfs:domain rdf:resource="http://purl.org/botsh-ext#BuildingInterface"/>
        <rdfs:range rdf:resource="http://purl.org/botsh-ext#BuildingElement"/>
        <rdfs:range rdf:resource="http://purl.org/botsh-ext#Zone"/>
        <rdfs:comment xml:lang="en">Relationship between an interface and another thing (building zone, element or owl:Thing)</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/botsh-ext#"/>
        <rdfs:label xml:lang="en">interface of</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/botsh-ext#intersectingElement -->


    <owl:ObjectProperty rdf:about="http://purl.org/botsh-ext#intersectingElement">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/botsh-ext#hasElement"/>
        <rdfs:comment xml:lang="en">Relation between a Zone and a building Element that intersects it.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/botsh-ext#"/>
        <rdfs:label xml:lang="en">intersecting element</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/botsh-ext#intersectsZone -->


    <owl:ObjectProperty rdf:about="http://purl.org/botsh-ext#intersectsZone">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/botsh-ext#Zone"/>
        <rdfs:range rdf:resource="http://purl.org/botsh-ext#Zone"/>
        <rdfs:comment xml:lang="en">Relationship between two zones whose 3D extent intersect. For example, a stairwell intersects different storeys.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/botsh-ext#"/>
        <rdfs:label xml:lang="en">intersects zone</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
        <schema:domainIncludes rdf:resource="http://purl.org/botsh-ext#Building"/>
        <schema:domainIncludes rdf:resource="http://purl.org/botsh-ext#BuildingStorey"/>
        <schema:domainIncludes rdf:resource="http://purl.org/botsh-ext#Site"/>
        <schema:domainIncludes rdf:resource="http://purl.org/botsh-ext#Space"/>
        <schema:rangeIncludes rdf:resource="http://purl.org/botsh-ext#Building"/>
        <schema:rangeIncludes rdf:resource="http://purl.org/botsh-ext#BuildingStorey"/>
        <schema:rangeIncludes rdf:resource="http://purl.org/botsh-ext#Site"/>
        <schema:rangeIncludes rdf:resource="http://purl.org/botsh-ext#Space"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/botsh-ext#isPartOf -->


    <owl:ObjectProperty rdf:about="http://purl.org/botsh-ext#isPartOf">
        <rdfs:domain rdf:resource="http://purl.org/botsh-ext#Thing"/>
        <rdfs:range rdf:resource="http://purl.org/botsh-ext#ArchitecturalElement"/>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/botsh-ext#"/>
        <ns1:term_status>stable</ns1:term_status>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/botsh-ext#linkHasInterface -->


    <owl:ObjectProperty rdf:about="http://purl.org/botsh-ext#linkHasInterface">
        <rdfs:domain rdf:resource="http://purl.org/toco/Link"/>
        <rdfs:range rdf:resource="http://purl.org/toco/Interface"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/botsh-ext#usesNetworkCommunicationDevice -->


    <owl:ObjectProperty rdf:about="http://purl.org/botsh-ext#usesNetworkCommunicationDevice">
        <rdfs:domain rdf:resource="http://purl.org/botsh-ext#Thing"/>
        <rdfs:range rdf:resource="http://purl.org/toco/Device"/>
    </owl:ObjectProperty>
    


