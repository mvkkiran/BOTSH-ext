<?xml version="1.0"?>
<rdf:RDF xmlns="http://purl.org/botsh-ext#"
     xml:base="http://purl.org/botsh-ext"
     xmlns:ns="http://www.w3.org/2006/vcard/ns#"
     xmlns:ns1="http://www.w3.org/2003/06/sw-vocab-status/ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:vann="http://purl.org/vocab/vann/"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:schema="https://schema.org/">
    <owl:Ontology rdf:about="http://purl.org/botsh-ext">
        <terms:created>2024-07-14</terms:created>
        <terms:creator xml:lang="en">Ramesh Gowtham</terms:creator>
        <terms:creator xml:lang="en">Vamsee Krishna Kiran Muppavarapu</terms:creator>
        <terms:description xml:lang="en">BOTSH Ontology extended with concepts related to Building and Non-Building structures with alignments to ToCo and ML-Schema ontologies.</terms:description>
        <terms:modified>2024-12-02</terms:modified>
        <terms:title xml:lang="en">Building Topology Ontology for Smart Homes (botsh-ext)</terms:title>
        <rdfs:comment xml:lang="en">Building Topology Ontology for Smart Homes - extended. BOTSH ontology is extended and aligned with ToCo communication ontology and ML-Schema ontology. Developed as part of PhD research work.</rdfs:comment>
        <owl:versionInfo>v.1.0</owl:versionInfo>
    </owl:Ontology>
    


